<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="book.MainPageController">
   <children>
       <Button fx:id="importBtn" layoutX="1.0" layoutY="1.0" mnemonicParsing="false" onAction="#onImportClick" text="Import" />
       <Button fx:id="insertBtn" layoutX="1.0" layoutY="1.0" mnemonicParsing="false" onAction="#onImportClick" text="Insert" />
       <TableView fx:id="booksTable" layoutX="105.0" layoutY="60.0" prefHeight="200.0" prefWidth="300.0">
        <columns>
            <TableColumn prefWidth="75.0" text="Name" fx:id="nameCol" />
            <TableColumn prefWidth="75.0" text="Author" fx:id="authorCol" />
            <TableColumn prefWidth="75.0" text="Release date" fx:id="releaseDateCol" />
            <TableColumn prefWidth="75.0" text="Price" fx:id="priceCol" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
